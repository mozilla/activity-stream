    "pontoon-to-json": "2.0.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.3.2",
    "react-intl": "2.2.3",
    "requirejs": "2.3.2",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.1",
    "shelljs": "0.7.6",
    "simple-git": "1.65.0",
